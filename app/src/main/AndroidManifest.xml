<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evrekaguys.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_emua"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
                android:parentActivityName=".StartActivity">
                <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".StartActivity">
                </meta-data>
        </activity>
        <activity android:name=".GaleriOlusturmaActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity">
            </meta-data></activity>
        <activity android:name=".LicenceScreen" android:label="Licence Screen"
            android:parentActivityName=".StartActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".StartActivity">
            </meta-data></activity>
        <activity android:name=".DetailsActivity" />
        <activity
            android:name=".SwipeActivity"
            android:screenOrientation="landscape"
            android:parentActivityName=".GaleriOlusturmaActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GaleriOlusturmaActivity">
            </meta-data>
        </activity>
    </application>

</manifest>